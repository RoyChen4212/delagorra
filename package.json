{
  "name": "Cherry",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "run:android": "cross-env-shell ENVFILE=.env.dev \"cd ./android && ./gradlew installDebug\"",
    "run:androidRelease": "cross-env-shell ENVFILE=.env.pro \"cd ./android && ./gradlew installRelease\"",
    "android:prod": "cross-env-shell ENVFILE=.env.pro \"cd ./android && ./gradlew assembleRelease\"",
    "android:prodBundle": "cross-env-shell ENVFILE=.env.pro \"cd ./android && ./gradlew bundleRelease\"",
    "run:ios": "npx react-native run-ios --scheme \"Development\" --simulator \"iPhone 12\"",
    "run:ios8": "npx react-native run-ios --scheme \"Development\" --simulator \"iPhone 8\"",
    "ios:prod": "npx react-native run-ios --scheme \"Production\" --simulator \"iPhone 12\"",
    "test": "jest",
    "lint": "eslint .",
    "postinstall": "cd node_modules/react-native-gifted-chat && yarn install && yarn lint && yarn build && yarn config:npm && rm -rf node_modules"
  },
